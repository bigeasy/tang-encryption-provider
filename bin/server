#!/bin/sh

export TANG_KMS_LOG_LEVEL=debug
export TANG_KMS_TANG_URL=http://127.0.0.1:8080
export TANG_KMS_THUMBPRINTS=2P5B1BrEu6ltBlfu8EWHUVxAJX6FRLCmTQUPsAHySa8/gWOHi5Tdcu10RKlTNOo7S8VvdN4a580N07dmU7HDqsM
